 var endYear = new Date('12-10-2020'); // Set day and month\r\n          endYear.setFullYear(date.getFullYear()); // Set year to this year\r\n          var secondsLeftPred = (endYear.getTime() - date.getTime())/ (1000);\r\n          var secondsLeft = Math.floor(secondsLeftPred);\r\n          var diffseconds = secondsLeft - this.CountMinutes*60 -this.CountHours*3600 - this.CountDate*24*3600\r\n          setInterval(() => {\r\n        return(diffseconds);\r\n    }, 1000)\r\n\r\n        },\r\n    },\r\n    methods:{\r\n    },\r\n}\r\n\r\n\r\n\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n  .countdown{\r\n    margin-top: 8%;\r\n    position: relative;\r\n    z-index: 2;\r\n    display: flex;\r\n    justify-content: flex-start;\r\n    align-items: center;\r\n  }\r\n  .time-section{\r\n    margin-right: 30px;\r\n    text-transform: uppercase;\r\n    color:#fff;\r\n    font-weight: 900;\r\n    font-size: 30px;\r\n    display: flex;\r\n    justify-content: space-berween;\r\n    align-items:center;\r\n    flex-direction: column;\r\n     @media(max-width: 720px){\r\n      font-size: 20px;\r\n    }\r\n    & .count{\r\n      font-size: 50px;\r\n    }\r\n    & #days{\r\n      color: pink;\r\n    }\r\n    & #hours{\r\n      color: lime;\r\n    }\r\n    & #minutes{\r\n      color: skyblue;\r\n    }\r\n    & #seconds{\r\n      color:green;\r\n    }\r\n  }\r\n</style>"]}]}